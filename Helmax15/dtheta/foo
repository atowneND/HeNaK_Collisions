#!/usr/local/gnuplot
    set term x11 0
    set xrange [0:50]
    plot 'barfile' u 2:(200*abs($5)/($3+$4))
    set xlabel 'jbar'; set ylabel '% diff Bmax';set title 'l=15-l=10 Bmax vs. jbar for dtheta_dj40.dat';
    replot
    set term postscript
    set output 'diffcompBmax_dj40.ps'
    replot
    quit
