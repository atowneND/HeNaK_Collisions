
    set term postscript
    set samples 10000
    plot 'stats_dj01.dat' using 1:4 with lines smooth csplines title 'SCstd fit'
    replot 'stats_dj01.dat' using 1:4 title 'SCstd'
    replot 'stats_dj01.dat' using 1:7 with lines smooth csplines title 'QMstd fit'
    replot 'stats_dj01.dat' using 1:7 title 'QMstd'
    set xlabel 'j';set ylabel 'alpha (deg)'; set title 'dj=1'
    
    set output 'std_dj01.ps'
    replot

    set term postscript
    set samples 10000
    plot 'stats_dj01.dat' using 1:10 with lines smooth csplines title 'diffs fit'
    replot 'stats_dj01.dat' using 1:10 title 'diffs'
    set output 'stddiffs_dj01.ps'
    replot
    
