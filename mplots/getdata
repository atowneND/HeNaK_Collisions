#!/bin/bash
j=1
dj=1
max=50
srcdir=/home/ashley/Documents/LeHigh/FortranResources/newdata/Ep002mf5i-50b.blam

while [ $dj -lt $(($max-1)) ]; do
    echo running dj=$dj
    while [ $j -lt $(($max-$dj+1)) ]; do
        jp=$(($j+$dj))
        ofile=jmp_j$j\_jp$jp\_dj$dj\.dat
        echo $j $jp|../calcmm $srcdir $ofile>/dev/null
        let j=j+1
    done
    j=1
    let dj=dj+1
done
