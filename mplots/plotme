#!/usr/local/gnuplot
set term x11 0
set samples 10000
set pointsize 4
plot 'jmp_j8_jp12_dj4.dat' u 2:($1==-12?($3+10*-12):1/0) with lines lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-12&&$2==-12?($3+10*-12):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-11?($3+10*-11):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-11&&$2==-11?($3+10*-11):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-10?($3+10*-10):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-10&&$2==-10?($3+10*-10):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-9?($3+10*-9):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-9&&$2==-9?($3+10*-9):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-8?($3+10*-8):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-8&&$2==-8?($3+10*-8):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-7?($3+10*-7):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-7&&$2==-7?($3+10*-7):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-6?($3+10*-6):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-6&&$2==-6?($3+10*-6):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-5?($3+10*-5):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-5&&$2==-5?($3+10*-5):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-4?($3+10*-4):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-4&&$2==-4?($3+10*-4):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-3?($3+10*-3):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-3&&$2==-3?($3+10*-3):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-2?($3+10*-2):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-2&&$2==-2?($3+10*-2):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-1?($3+10*-1):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==-1&&$2==-1?($3+10*-1):1/0) pt 6 ps 3 lt rgb 'red' title ''
replot 'jmp_j8_jp12_dj4.dat' u 2:($1==0?($3+10*0):1/0) with lines lt rgb 'red' title ''
    replot 'jmp_j8_jp12_dj4.dat' u 2:($1==0&&$2==0?($3+10*0):1/0) pt 6 ps 3 lt rgb 'red' title ''
set xlabel 'mprime'; set ylabel 'magnitude'
set title 'propensity for m to transition to mprime'
