#!/bin/bash
args='getopt j:p:d $*'
j=0;jp=0;dj=0
echo parsing...
for i
do
    case "$i" in
        -j) shift; echo "j=$1";shift
            ;;
        -p) shift; echo "jp=$1";shift
            ;;
        -d) shift; echo "dj=$1";shift
            ;;
    esac
done

if [ $dj -ne $(($jp-$j)) ] ; then
    echo bad input values
    echo dj=$dj; j=$j; jp=$jp
    exit
fi

j=16
jp=18
if [ $jp -lt 10 ]; then
    jpstr=0$jp
else
    jpstr=$jp
fi
if [ $j -lt 10 ]; then
    jstr=0$j
else
    jstr=$j
fi
blamdir=Run2Results/Blambdas/
blamdat=Blambda_$jstr\_$jpstr\.dat

foo=$(find $blamdir -name $blamdat -nowarn)
if [ "$foo" = "$blamdir$blamdat" ]; then
    echo found it
else
    echo not here
fi

gnuplot < fooplot > /dev/null
